<UserControl x:Class="Quickr.Views.Server.InfoView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Quickr.Views.Server"
             xmlns:viewModels="clr-namespace:Quickr.ViewModels"
             xmlns:server="clr-namespace:Quickr.ViewModels.Server"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignInstance server:InfoViewModel}">
    <Grid>
        <local:KeyValueGroupView DataContext="{Binding Info}"/>
    </Grid>
</UserControl>
